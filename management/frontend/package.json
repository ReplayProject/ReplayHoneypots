{
  "name": "replay-frontend",
  "version": "0.0.1",
  "description": "A tiny VueJS app bundled using ParcelJS && Pouch Database & Frontend",
  "main": "index.html",
  "scripts": {
    "build": "parcel build ./client/index.html --public-url 'http://152.14.85.156:8080' ",
    "build-watch": "parcel watch ./client/index.html --public-url 'http://152.14.85.156:8080' ",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
    "format": "npx prettier-standard && npx sort-package-json ",
    "start": "NODE_ENV=development npm run build && DEBUG_HIDE_DATE=true DEBUG=sd:*,-sd:history node server/index.js",
    "start-watch": "DEBUG_HIDE_DATE=true DEBUG=sd:*,-sd:history nodemon --ignore config.json server/index.js",
    "test": "npm run format && npx nyc ava test/specs/*"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "ava": {
    "require": [
      "./test/_setup.js"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chart.js": "^2.9.3",
    "concurrently": "^5.1.0",
    "connect-ensure-login": "^0.1.1",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-pouchdb": "^4.2.0",
    "express-session": "^1.16.1",
    "express-ws": "^4.0.0",
    "helmet": "^3.21.3",
    "memdown": "^5.1.0",
    "node-pty": "^0.9.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pouch-vue": "^0.3.5",
    "pouchdb": "^7.2.1",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-live-find": "^0.4.0",
    "serve-static": "^1.14.1",
    "supports-color": "^7.1.0",
    "tachyons": "^4.11.1",
    "uuid": "^7.0.2",
    "vue": "latest",
    "vue-good-table": "^2.19.1",
    "vue-progressbar": "^0.7.5",
    "vue-router": "latest",
    "vue-toasted": "^1.1.28",
    "vuejs-datatable": "^2.0.0-alpha.7",
    "xterm": "^4.4.0",
    "xterm-addon-attach": "^0.5.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-web-links": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@vue/component-compiler-utils": "^3.1.1",
    "@vue/test-utils": "^1.0.0-beta.32",
    "ava": "^3.3.0",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "parcel-bundler": "^1.12.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0",
    "sass": "^1.26.2",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "engines": {
    "node": "v12.0.0"
  }
}
