{
  "name": "replay-frontend",
  "version": "0.0.1",
  "description": "A tiny VueJS app bundled using ParcelJS",
  "main": "index.html",
  "scripts": {
    "start": "NODE_ENV=development parcel serve index.html --port 3000",
    "build": "parcel build index.html",
    "format": "npx prettier-standard ./* ./src/* src/**/* test/**/*",
    "test": "npm run format && npx nyc ava test/specs/*"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.9.3",
    "pouch-vue": "^0.3.5",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-browser": "^7.2.1",
    "pouchdb-find": "^7.2.1",
    "pouchdb-live-find": "^0.4.0",
    "tachyons": "^4.11.1",
    "vue": "latest",
    "vue-progressbar": "^0.7.5",
    "vue-router": "latest",
    "xterm": "^4.4.0",
    "xterm-addon-attach": "^0.5.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-web-links": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@vue/component-compiler-utils": "^3.1.1",
    "ava": "^3.3.0",
    "jsdom": "^16.2.0",
    "jsdom-global": "^3.0.2",
    "parcel-bundler": "^1.12.4",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0",
    "sass": "^1.26.2",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "ava": {
    "require": [
      "./test/_setup.js"
    ]
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  },
  "browserslist": [
    "last 1 Chrome version"
  ]
}
